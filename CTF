#!/bin/bash
(
 flock -n 9 || exit 1
 echo "beaconing"
 for i in $(seq 1 5); do nc -lw10 127.0.0.1 -p 1234 2>/dev/null  ; sleep 10; done
 echo "done beaconing"
) 9>/tmp/mylockfile

Mausan ukoul for avhe mubullat goth,witch_king,127.0.0.1:1234


